PACKAGE_NAME="i40e"
PACKAGE_VERSION="#MODULE_VERSION#"
AUTOINSTALL=yes
REMAKE_INITRD=no
BUILT_MODULE_NAME="i40e"
DEST_MODULE_LOCATION="/updates/dkms"
MAKE="make -C ${kernel_source_dir} V=1 M=${dkms_tree}/${PACKAGE_NAME}/${PACKAGE_VERSION}/build"
CLEAN="make -C ${kernel_source_dir} V=1 M=${dkms_tree}/${PACKAGE_NAME}/${PACKAGE_VERSION}/build clean"
